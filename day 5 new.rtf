5)  Return all the palindromes in an array\par
\par
function isPalindrome(N)\par
    \{\par
        let str = "" + N;\par
        let len = str.length;\par
        for (let i = 0; i < parseInt(len / 2, 10); i++)\par
        \{\par
            if (str[i] != str[len - 1 - i ])\par
                return false;\par
        \}\par
        return true;\par
    \}\par
    \par
  Anonymous Function :  function (arr, n)\par
    \{\par
        // Traversing each element of the array\par
        // and check if it is palindrome or not\par
        for (let i = 0; i < n; i++)\par
        \{\par
            let ans = isPalindrome(arr[i]);\par
            if (ans == false)\par
                return false;\par
        \}\par
        return true;\par
    \}\par
